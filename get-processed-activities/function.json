{
  "disabled": false,
  "bindings": [
    {
      "route": "processedactivities/{userid}",
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "tableName": "StravaWeatherman",
      "name": "processedActivities",
      "type": "table",
      "partitionKey": "processedActivities",
      "filter": "userId eq {userid}",
      "direction": "in"
    },
    {
      "tableName": "StravaWeatherman",
      "name": "userTokens",
      "type": "table",
      "partitionKey": "tokenToUser",
      "filter": "userId eq {userid}",
      "direction": "in"
    }
  ]
}